{
  "name": "element-plus-kit",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.17.0",
  "license": "Apache-2.0",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "prepare": "husky",
    "dev": "turbo run dev --filter=./packages/* --filter=./playground",
    "dev:packages": "turbo run dev --filter=./packages/*",
    "dev:playground": "turbo run dev --filter=./playground",
    "build:packages": "turbo run build --filter=./packages/*",
    "build": "pnpm build:packages && pnpm type-check",
    "type-check": "vue-tsc --build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "eslint:inspect": "eslint-config-inspector --config ./eslint.config.js",
    "changeset": "changeset",
    "version": "pnpm build && changeset version",
    "release": "changeset publish",
    "docs:dev": "pnpm --filter ./docs dev",
    "docs:build": "pnpm run build && pnpm --filter ./docs build",
    "docs:preview": "pnpm run build && pnpm --filter ./docs preview"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/config-inspector": "^1.4.2",
    "@iswangh/commitlint-config": "^0.2.0",
    "@iswangh/eslint-config": "^0.2.3",
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "sass": "^1.97.1",
    "turbo": "^2.7.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1"
  },
  "volta": {
    "node": "25.2.1"
  }
}
