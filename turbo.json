{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["tsconfig.json", "tsconfig.app.json"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
