{
  "name": "@iswangh/element-plus-kit",
  "type": "module",
  "version": "0.1.1",
  "description": "Element Plus Kit - 基于 Element Plus 的 Vue 3 组件集合",
  "author": "iswangh",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/iswangh/element-plus-kit.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "element-plus",
    "components",
    "form",
    "ui"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "import": "./dist/resolver.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "vue-tsc --noEmit",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "prepublishOnly": "pnpm build && pnpm type-check"
  },
  "peerDependencies": {
    "element-plus": "^2.11.7",
    "vue": "^3.5.23"
  },
  "dependencies": {
    "@iswangh/element-plus-kit-form": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.23",
    "vue-tsc": "^3.0.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
