{
  "name": "@iswangh/element-plus-kit-core",
  "type": "module",
  "version": "0.1.3",
  "description": "Element Plus Kit 核心工具函数包",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "vue-tsc --noEmit",
    "prepublishOnly": "pnpm type-check && pnpm build",
    "publish:patch": "pnpm version patch --no-git-tag-version && pnpm publish",
    "publish:minor": "pnpm version minor --no-git-tag-version && pnpm publish",
    "publish:major": "pnpm version major --no-git-tag-version && pnpm publish"
  },
  "peerDependencies": {
    "vue": "^3.5.23"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
