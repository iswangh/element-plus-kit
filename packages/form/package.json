{
  "name": "@iswangh/element-plus-kit-form",
  "type": "module",
  "version": "0.1.3",
  "description": "Element Plus Kit 表单组件包",
  "license": "Apache-2.0",
  "sideEffects": [
    "./dist/index.js",
    "./dist/style.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "sideEffects": [
        "./dist/index.js",
        "./dist/style.css"
      ]
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "vue-tsc --noEmit",
    "prepublishOnly": "pnpm type-check && pnpm build",
    "publish:patch": "pnpm version patch --no-git-tag-version && pnpm publish",
    "publish:minor": "pnpm version minor --no-git-tag-version && pnpm publish",
    "publish:major": "pnpm version major --no-git-tag-version && pnpm publish"
  },
  "peerDependencies": {
    "element-plus": "^2.11.7",
    "vue": "^3.5.23"
  },
  "dependencies": {
    "@iswangh/element-plus-kit-core": "workspace:*"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "element-plus": "^2.11.8",
    "sass": "^1.94.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
